{
  "name": "@builtwithluv/mockit",
  "version": "1.4.8",
  "description": "Tool for mocking services",
  "main": "lib/index.js",
  "scripts": {
    "build": "npx webpack --config ./webpack.prod.js -p && npx babel src/server -d lib/server && npx babel src/commands -d lib/commands && npx babel src/index.js --out-file lib/index.js",
    "prepublishOnly": "npx cleandir node_modules && npm i && npm run build",
    "start": "npx concurrently --kill-others --exec babel-node \"npm run start:server-dev\" \"npm run start:client\"",
    "start:client": "npx webpack-dev-server --config ./webpack.dev.js --mode development",
    "start:server": "npx babel-node example/server/index.js",
    "start:server-dev": "npx nodemon example/server/index.js --exec babel-node",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "npx concurrently --kill-others \"npm run start:server\" \"npx cypress run\"",
    "test:unit": "jest"
  },
  "bin": "lib/.bin/mockit",
  "author": "Cheng Ly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/builtwithluv/mockit"
  },
  "files": [
    "lib",
    "example"
  ],
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/example/",
      "<rootDir>/lib/",
      "<rootDir>/node_modules/",
      "<rootDir>/tests/"
    ],
    "moduleNameMapper": {
      "^@(/.+)": "<rootDir>$1",
      "^@client(/.+)": "<rootDir>/src/client$1",
      "^@server(/.+)": "<rootDir>/src/server$1"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/tests/mocks/"
    ],
    "roots": [
      "<rootDir>/tests/"
    ],
    "setupFiles": [
      "<rootDir>/tests/helpers/setup.js"
    ],
    "testURL": "http://localhost/"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.7.0",
    "@blueprintjs/icons": "^3.2.0",
    "@blueprintjs/select": "^3.2.1",
    "@material-ui/core": "^3.3.1",
    "@material-ui/icons": "^3.0.1",
    "classnames": "^2.2.6",
    "cors": "^2.8.4",
    "express": "^4.16.4",
    "js-beautify": "^1.8.8",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "mkdir-recursive": "^0.4.0",
    "node-fetch": "^2.1.2",
    "node-watch": "^0.5.8",
    "nodemon": "^1.18.7",
    "pug": "^2.0.3",
    "re-resizable": "^4.9.2",
    "react": "^16.6.0",
    "react-ace": "^6.2.0",
    "react-code-prettify": "0.0.1",
    "react-dom": "^16.6.0",
    "typeface-roboto": "^0.0.54",
    "uuid": "^3.3.2",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@cypress/webpack-preprocessor": "^3.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "clean-dir": "^1.0.3",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.1",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "^1.0.0",
    "cypress": "^3.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pug-loader": "^2.4.0",
    "react-hot-loader": "^4.3.11",
    "regenerator-runtime": "^0.12.1",
    "style-loader": "^0.21.0",
    "superagent": "^3.8.3",
    "supertest": "^3.3.0",
    "webpack": "^4.23.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  }
}
