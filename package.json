{
  "name": "@builtwithluv/testy",
  "version": "0.0.2",
  "description": "Framework for mocking",
  "main": "lib/index.js",
  "scripts": {
    "build": "npx webpack --config ./webpack.prod.js -p",
    "prepublishOnly": "npx cleandir node_modules && npm i && npm run build",
    "start": "npx concurrently --kill-others \"npm run start:server\" \"npm run start:client\"",
    "start:server": "./node_modules/.bin/nodemon example/server/index.js",
    "start:client": "./node_modules/.bin/webpack-dev-server --config ./webpack.dev.js --mode development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Cheng Ly",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/builtwithluv/testy"
  },
  "files": [
    "lib",
    "example"
  ],
  "dependencies": {
    "@material-ui/core": "^1.2.2",
    "@material-ui/icons": "^1.1.0",
    "classnames": "^2.2.6",
    "express": "^4.16.3",
    "lodash.flow": "^3.5.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "typeface-roboto": "0.0.54",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-dir": "^1.0.3",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.6.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.5",
    "prop-types": "^15.6.2",
    "react-hot-loader": "^4.3.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "engines": {
    "node": ">=5.0.0"
  }
